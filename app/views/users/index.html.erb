<!-- View for index action in user's controleer -->
<% provide(:title, 'All users') %>
<h2>All users</h2>
<br /><br />

<ul class="users">
  <% @users.each do |user| %>
<li>
	<p><%= link_to user.user_name, user %> (<%= user.first_name %> <%= user.last_name %>) </p>
      <%= user.email %> <br />
      <a href="#">+ Add Friend</a>

</li>

  <% end %>
</ul>